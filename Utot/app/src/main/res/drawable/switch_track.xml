<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:right="2dp" android:bottom="2dp" android:state_checked="false">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="rectangle" android:padding="10dp">
                <corners
                    android:bottomLeftRadius="20dp"
                    android:bottomRightRadius="20dp"
                    android:topLeftRadius="20dp"
                    android:topRightRadius="20dp"/>
                <stroke android:color="@color/switch_button_unchecked" android:width="3dp"/>

            </shape>
        </item>
    <item android:right="2dp" android:bottom="2dp" android:state_checked="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle" android:padding="10dp">
            <corners
                android:bottomLeftRadius="20dp"
                android:bottomRightRadius="20dp"
                android:topLeftRadius="20dp"
                android:topRightRadius="20dp"/>
            <solid android:color="@color/switchbutton_checked"/>
            </shape>
    </item>


    </selector>